{
  "name": "@chakra-ui/storybook-addon",
  "version": "0.0.0",
  "description": "The official Storybook Addon for Chakra UI",
  "keywords": [
    "storybook-addons",
    "chakra",
    "addon"
  ],
  "author": "Mark Chandler <mark@chanchan.io>",
  "homepage": "https://github.com/chakra-ui/chakra-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chakra-ui/chakra-ui.git"
  },
  "bugs": {
    "url": "https://github.com/chakra-ui/chakra-ui/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "preset.js"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "concurrently yarn:build:*",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --root-mode upward --extensions .ts,.tsx -d dist --source-maps",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types"
  },
  "dependencies": {},
  "devDependencies": {},
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
